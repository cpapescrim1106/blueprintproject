         <property name="locations">
            <list>                
                <value>${javaws.codebase}shared/clientConfig_generic.properties</value>
                <value>${javaws.codebase}clientConfig_base.properties</value>
                <value>${javaws.codebase}clientConfig.properties</value>
            </list>
        </property>
    </bean>
        
    <bean id="propertyConfigurer" class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
         <property name="properties" ref="properties"/>
    </bean>
   
    <!-- CodeBase (for non-JavaWS testing purposes only) -->
