    <!-- Online code version. If this is higher than the client's version, the client should exit and invoke the launch URL to update the locally deployed code -->
    <bean id="codeVersion" class="java.lang.String">
        <constructor-arg value="4.8.1"/>
    </bean>
    
    <bean id="bplinkCodeVersion" class="java.lang.String">
        <constructor-arg value="4.1"/>
    </bean>    
  
    <!-- Property place holder location -->
